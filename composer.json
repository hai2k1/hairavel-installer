{
  "name": "haibase/hairavel-installer",
  "description": "Hairavel installer",
  "license": "MIT",
  "authors": [
    {
        "name": "HaiPHP",
        "email": "trihai306201@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "autoload": {
    "psr-4": {
      "haibase\\HairavelInstaller\\": "src/"
    },
    "files": [
      "src/Helpers/functions.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "haibase\\HairavelInstaller\\Providers\\HairavelInstallerServiceProvider"
      ]
    },
    "hairavel": {
      "event": [
        "haibase\\HairavelInstaller\\Events\\InstallSeed"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
